{
  "repo_full_name": "nickname/repo",
  "github_token": "token",
  "filter_mode": "exclude",
  "filter_branches": ["exclude-this-branch"],
  "branches_dir": "branches",
  "commands": {
    "start": "npm start -- -H {host} -p {port}",
    "build": [
      "npm install",
      "npm run build"
    ],
    "update": [
      ""
    ]
  },
  "expose": {
    "enabled": true,
    "port": "auto",
    "host": "127.0.0.1"
  },
  "webhook_update": true,
  "auto_add_webhook": true,
  "clear_redundant_local_branches": true,
  "enabled_log_levels": ["all"]
}